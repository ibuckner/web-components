{
  "name": "@buckneri/web-components",
  "version": "0.1.7",
  "description": "Web components using Stencil",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/web-components/web-components.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "dev.listglobals": "npm list -g --depth 0",
    "generate": "stencil generate",
    "prepare": "stencil build --prod --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "upload": "npm run prepare && npm publish --access public",
    "upload.test": "npm publish --dry-run --access public"
  },
  "devDependencies": {
    "@stencil/core": "^1.5.4",
    "@types/d3": "^5.7.2",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.11",
    "@types/puppeteer": "^1.20.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "puppeteer": "^1.20.0"
  },
  "license": "MIT",
  "dependencies": {
    "@buckneri/js-lib-color": "0.0.4",
    "d3-interpolate": "^1.3.2",
    "d3-shape": "^1.3.5",
    "resize-observer": "^1.0.0"
  },
  "bugs": {
    "url": "https://github.com/ibuckner/web-components/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibuckner/web-components.git"
  }
}
